{% extends 'layouts/page.njk' %}

{% block breadcrumb %}
<li>
    <span>全民经纪</span>
    <i class="fa fa-circle"></i>
</li>
<li>
    <span>订单管理</span>
</li>
{% endblock %}

{% block title %}订单管理{% endblock %}

{% block note %}查看所有订单信息{% endblock %}

{% block table %}
{% from "kits/dtb.njk" import dtb %}
{{
    dtb([
        {value: '订单号', name: 'id'},
        {width: '5%', value: '手机号', name: 'mobile'},
        {width: '10%', value: '状态'},
        {value: '是否补充资料'}
    ])
}}
{% endblock %}

{% block js %}
<script src="/metronic/global/plugins/bootstrap-confirmation/bootstrap-confirmation.js"></script>
<script src="/js/order/defaults.js"></script>
<script src="/js/order/page.js"></script>
{% endblock %}